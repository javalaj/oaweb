package com.thinkgem.jeesite.modules.oa.entity.mytask;

import com.thinkgem.jeesite.common.persistence.DataEntity;

/**
 * 会议状态回执记录Entity
 * @author cz
 * @version 2017-01-03
 */
public class MeetingRecord extends DataEntity<MeetingRecord>{
	private static final long serialVersionUID = 1L;
	private String planId;		// plan_id
	private String userId;		// user_id
	private String state;		// 状态(0:未回执，1:回执)
	
	public MeetingRecord() {
		super();
	}

	public MeetingRecord(String id){
		super(id);
	}

	public String getPlanId() {
		return planId;
	}

	public void setPlanId(String planId) {
		this.planId = planId;
	}
	
	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}
	
	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}
	
}
