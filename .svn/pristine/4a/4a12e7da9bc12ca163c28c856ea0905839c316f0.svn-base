@CHARSET "UTF-8";

.control-group > label {
	text-align: right;
	width: 20%;
	margin-right: 2%;
}

.form-actions {
	margin-left: 23%;
}

.control-group1 ol {
	margin-left: 23%;
}

.control-group1 .btn-primary {
	margin-left: 23%;
}
/*流程意见div，文字太长时，格式排布*/
.lc-view-div {
	margin-left: 23%;
	margin-top: -19px;
	word-wrap: break-word;
	word-break: break-all;
}
/*必填选项 label异常*/
label.error {
	color: #cc5965;
	display: inline-block;
	margin: 0px;
	text-align: left;
	width: auto;
}